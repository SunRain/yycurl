                            if(parseFloat(flag) < 0) {
                                obj.className = 'num down';
                            } else if(parseFloat(flag) > 0) {
                                obj.className = 'num up';
                            } else {
                                obj.className = 'num';
                            }
                            obj.innerHTML = val;
                        }

