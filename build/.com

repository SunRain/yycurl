(["http://img.adbox.sina.com.cn/ad/15640.html", ""]);

<!--垫底广告结束-->
new RotatorAD(rad,nad,'ad_edu_04');

}
});
}
});

</script>
<!--教育文字链 end-->
<!--ad end-->
</div>
